<Pane fx:id="room" focusTraversable="true" onKeyPressed="#onKeyPressed" onKeyReleased="#onKeyReleased" prefHeight="450.0" prefWidth="636.42857" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nz.ac.auckland.se206.controllers.RoomController">
   <children>
      <ImageView fitHeight="450.0" fitWidth="636.42857">
         <image>
            <Image url="@../images/room.png" />
         </image>
      </ImageView>
      <Rectangle fx:id="rectangleDiary" arcHeight="5.0" arcWidth="5.0" fill="#1f93ff" focusTraversable="true" height="25.0" layoutX="267.0" layoutY="324.0" onMouseClicked="#onClickDiary" opacity="0.0" stroke="BLACK" strokeType="INSIDE" width="46.0" />
      <Rectangle fx:id="rectangleViolin" arcHeight="5.0" arcWidth="5.0" fill="#1f93ff" focusTraversable="true" height="80.0" layoutX="390.0" layoutY="259.0" onMouseClicked="#onClickViolin" opacity="0.0" stroke="BLACK" strokeType="INSIDE" width="52.0" />
