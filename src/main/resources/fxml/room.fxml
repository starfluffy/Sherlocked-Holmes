         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <Label fx:id="labelTimerBlack" alignment="CENTER" contentDisplay="CENTER" layoutX="544.0" layoutY="380.0" text="120" textAlignment="CENTER" textFill="#7b9cbf" visible="false">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
